{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "importHelpers": true,
    "declaration": true,
    "sourceMap": false,
    "target": "es6",
    "lib": ["es6"],
    "outDir": "../lib"
  }
}
